	"traitdesc-temporaltransmutation": "Has the ability to craft <a href=\"handbooksearch://temporal gear\">temporal gears</a>, <a href=\"handbooksearch://sulfur chunk\">sulfur</a>, <a href=\"handbooksearch://saltpeter\">saltpeter</a>, and <a href=\"handbooksearch://high fertility soil\">high fertility soil</a> via exclusive <a href=\"handbooksearch://temporal transmutation\">barrel</a> recipes",
	"traitdesc-artisan": "Exclusive craftable <a href=\"handbooksearch://wallpaper\">wallpapers</a>, and can glaze <a href=\"handbooksearch://Raw storage vessel\">Raw storage vessels</a>, <a href=\"handbooksearch://Raw flowerpot\">Raw flowerpots</a>, and <a href=\"handbooksearch://Raw planter\">Raw planters</a>",
	"traitdesc-culinary": "Exclusive craftable<a href=\"handbooksearch://vintage beef\">Vintage beef</a>, <a href=\"handbooksearch://cooking pot\">metal cooking pots</a>, and <a href=\"handbooksearch://frying pan\">metal frying pans</a>",
	"traitdesc-shelflifesavant": "Exclusive craftable <a href=\"handbooksearch://rosin\">rosin</a> that doubles the longevity of food sealed in <a href=\"handbooksearch://fired crock\">crocks</a>",
	"traitdesc-tinkerer": "Exclusive craftable <a href=\"handbooksearch://tuning spear\">device</a> to tame locusts, <a href=\"handbooksearch://metal parts\">metal parts</a>, better recipes for <a href=\"handbooksearch://chute\">chutes</a>, <a href=\"handbooksearch://archimedes\">archimedes screws</a>, <a href=\"handbooksearch://hopper\">hoppers</a>, <a href=\"handbooksearch://axle\">axles</a>, and <a href=\"handbooksearch://angled gear\">angled gears</a>",
	"traitdesc-locustlover": "+50% hp to hacked locusts, and has the ability to heal hacked locusts with <a href=\"handbooksearch://metal parts\">metal parts</a>",
	"traitdesc-tenacious": "Exclusive craftable <a href=\"handbooksearch://estoc\">Forlorn hope estoc</a>",
	"traitdesc-pioneer": "Exclusive craftable <a href=\"handbooksearch://booster\">soil boosters</a>, <a href=\"handbooksearch://seed\">seeds</a>, and a better recipe for <a href=\"handbooksearch://terra preta\">terra preta</a>",
	"traitdesc-carpenter": "Exclusive craftable <a href=\"handbooksearch://wagon\">wagon wheels</a>, <a href=\"handbooksearch://paper lantern\">paper lanterns</a>, <a href=\"handbooksearch://aged firewood\">aged firewood</a>, and better recipes for <a href=\"handbooksearch://stick\">sticks</a>, <a href=\"handbooksearch://firewood\">firewood</a>, and <a href=\"handbooksearch://board\">boards</a>",
	"traitdesc-mason": "Exclusive craftable <a href=\"handbooksearch://advanced bloomery\">advanced bloomery</a>, exclusive craftable <a href=\"handbooksearch://hardened\">Hardened clay blocks</a>, cheaper <a href=\"handbooksearch://plaster (plain)\">plaster</a>, cheaper <a href=\"handbooksearch://mortar\">mortar recipe</a>, cheaper <a href=\"handbooksearch://raw blue clay brick or raw red clay brick or raw fire clay brick\">raw clay brick recipes</a>, and cheaper <a href=\"handbooksearch://unfired\">unfired refractory brick recipes</a>",
	"traitdesc-packmule": "Exclusive craftable <a href=\"handbooksearch://peddler's pack\">peddler's pack</a>",
	"traitdesc-specialstock": "Able to access the past week's shop inventory for every trader",
	"traitdesc-silvertongue": "Cheaper prices for every trader",
	"traitdesc-prospector": "Exclusive craftable <a href=\"handbooksearch://reinforced sturdy leather mining bag\">reinforced sturdy leather mining bag</a>, <a href=\"handbooksearch://toolkit\">toolkits</a>, <a href=\"handbooksearch://mining lanterns\">mining lanterns</a>, <a href=\"handbooksearch://copper pan\">copper pan</a>, cheaper <a href=\"handbooksearch://blasting powder\">blasting powder</a> recipes, and cheaper <a href=\"handbooksearch://bomb\">bomb</a> recipes",
	"traitdesc-shelteredstone": "No temporal stability loss whilst underground, no matter how deep.",
	"traitdesc-mystic": "Exclusive craftable <a href=\"handbooksearch://crystal skull\">crystal skull</a>",
	"traitdesc-clairvoyance": "Able to use the <a href=\"handbooksearch://crystal skull\">crystal skull</a> to find broken static translocators",
	"traitdesc-smith": "Exclusive craftable <a href=\"handbooksearch://armor kit\">armor kits</a>, <a href=\"handbooksearch://metal barrel\">metal barrels</a> for the alchemist, <a href=\"handbooksearch://stainless steel mix\">stainless steel mix</a> to be used in the advanced bloomery, is the only one capable of interacting with the advanced bloomery",
	"traitdesc-recycler": "Can break down <a href=\"handbooksearch://rusty gear\">rusty gears</a>, <a href=\"handbooksearch://metal scraps\">metal scraps</a>, and <a href=\"handbooksearch://metal parts\">metal parts</a> for metals of that type",
	"traitdesc-tallowtanner": "Can recycle unused pelts into <a href=\"handbooksearch://scraped hide\">scraped hides</a>",

